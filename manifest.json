{
  "manifest_version": 2,
  "name": "Prism",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.2.0.min.js",
        "content.js"
      ]
    }
  ],
  "permissions": [
      "storage",
      "*://*/*"
  ],
  "browser_action": {
    "default_title": "Prism",
    "default_popup": "prism.html"
  },
  "icons": {
    "16": "assets/icon/icon_16.png",
    "24": "assets/icon/icon_24.png",
    "32": "assets/icon/icon_32.png",
    "64": "assets/icon/icon_64.png",
    "128": "assets/icon/icon_128.png"
  }
}
