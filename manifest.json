{
  "manifest_version": 2,
  "name": "Prism",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.2.0.min.js",
        "content.js"
      ]
    }
  ],
  "permissions": [
      "storage",
      "*://*/*"
  ],
  "browser_action": {
    "default_title": "Prism",
    "default_popup": "prism.html"
  },
  "icons": {
    "16": "assests/icons/"
  }
}
